{
  "name": "amethystjs",
  "version": "1.1.6",
  "main": "dist/index.js",
  "repository": "https://github.com/Greensky-gs/amethystJS",
  "author": "Greensky-gs <72513011+Greensky-gs@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.6.0",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "prettier": "prettier -w src index.d.ts",
    "eslint": "eslint --fix --ext ts src index.d.ts",
    "build": "tsc -b"
  },
  "files": [
    "./dist",
    "./index.d.ts"
  ],
  "types": "./index.d.ts",
  "directories": {
    "lib": "src"
  },
  "bugs": {
    "url": "https://github.com/Greensky-gs/amethystJS/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./src/*": "./dist/*",
    "./dist/*": "./dist/*"
  },
  "description": "A complete framework for Discordjs v14",
  "keywords": [
    "discord",
    "discordjs",
    "discord-js",
    "framework"
  ]
}
